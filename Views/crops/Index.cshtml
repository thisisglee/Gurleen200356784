@model IEnumerable<Gurleen200356784.Models.crop>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cropid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cropname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.seasonal)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cropid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cropname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.seasonal)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cropid }) |
